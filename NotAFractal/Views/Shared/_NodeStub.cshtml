@model NotAFractal.Models.ViewModels.NodeViewModel

<div class="node" data-open="false" data-generated="false" data-seed="@Model.Seed" data-Type="@Model.Type">
    @if(Model.EmptyNode) 
    {
        <span class="">            
            @Model.Title
        </span>  <a class="infoLink">Info</a>      
    }else 
    {        
        <span class="toggleLink">
            <span class="treeViewToggleIndicator btn btn-inverse btn-mini">+</span> 
            @Model.Title
        </span>  <a class="infoLink">Info</a>      
    }   
</div>
